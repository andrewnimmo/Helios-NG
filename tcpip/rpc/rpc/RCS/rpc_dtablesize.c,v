/*
 * Cache the result of getdtablesize(), so we don't have to do an
 * expensive system call every time.
 */
int
_rpc_dtablesize()
{
	static int size;
	
	if (size == 0) {
#ifdef __HELIOS
		size = 64;
#else
		size = getdtablesize();
#endif
	}
	return (size);
}
