@#ifndef __xioevents_h
#define __xioevents_h

/*
 * extensions to ioevents.h for X
 */
 
#define Buttons_Up		0x00008000L
#define Buttons_Button0		0x00000001L
#define Buttons_Button1		0x00000002L
#define Buttons_Button2		0x00000004L
#define Buttons_Button3		0x00000008L
#define Buttons_Button_Mask	0x0000000FL

#endif /* _x_ioevents_h */
