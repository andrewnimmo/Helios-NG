#ifdef lint
static char sccsid[] = "@@(#)clear.c	5.3 (Berkeley) 6/30/88";
#endif /* not lint */

# include	"curses.ext"

/*
 *	This routine clears the window.
 *
 */
int wclear(reg WINDOW	*win)
  {

	werase(win);
	win->_clear = TRUE;
	return OK;
}
@


