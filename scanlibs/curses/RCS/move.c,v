#ifdef lint
static char sccsid[] = "@@(#)move.c	5.4 (Berkeley) 6/30/88";
#endif /* not lint */

# include	"curses.ext"

/*
 *	This routine moves the cursor to the given point
 *
 */
int
wmove(
      reg WINDOW	*win,
      reg int		y,
      reg int		x)
{

# ifdef DEBUG
	fprintf(outf, "MOVE to (%d, %d)\n", y, x);
# endif
	if (x < 0 || y < 0)
		return ERR;
	if (x >= win->_maxx || y >= win->_maxy)
		return ERR;
	win->_curx = x;
	win->_cury = y;
	return OK;
}
@


